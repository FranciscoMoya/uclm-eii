<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE offerings PUBLIC "-//UniTime//DTD University Course Timetabling/EN" "http://www.unitime.org/interface/CourseOfferingExport.dtd">

{% set cursos = data %}

<offerings campus="UCLM" year="2019" term="{{term}}" dateFormat="yyyy/M/d" timeFormat="HHmm" created="Mon May 27 13:19:05 CEST 2019" includeExams="none">

  {% for c in cursos %}
  <offering id="{{ 200000 + c['uniqueid'] }}" offered="true" action="insert">
    <course id="{{ 210000 + c['uniqueid'] }}" subject="{{ c['subject'] }}" courseNbr="{{ c['nr'] }}" controlling="true" title="{{ c['asignatura'] }}" scheduleBookNote="">
      <courseCredit creditType="collegiate" creditUnitType="semesterHours" creditFormat="fixedUnit" fixedCredit="4.0"/>
    </course>
    <config name="1" limit="50" durationType="MIN_PER_WEEK">
      <subpart type="Lec" suffix="" minPerWeek="50"></subpart>
      <subpart type="Lec" suffix="a" minPerWeek="50"></subpart>
      <subpart type="Lec" suffix="b" minPerWeek="50"></subpart>
      <subpart type="Lab" suffix="" minPerWeek="100"></subpart>
      <class id="{{ c['subject'] }} {{ c['nr'] }} Lec 1" type="Lec" suffix="1" limit="50" studentScheduling="true" displayInScheduleBook="true" cancelled="false">
        {% for i in c['teoria'] %}
        <instructor id="{{i}}" share="{{100//len(c['teoria'])}}"/>
        {% endfor %}
      </class>
      <class id="{{ c['subject'] }} {{ c['nr'] }} Lec 1a" type="Lec" suffix="1a" limit="50" studentScheduling="true" displayInScheduleBook="true" cancelled="false">
        {% for i in c['teoria'] %}
        <instructor id="{{i}}" share="{{100//len(c['teoria'])}}"/>
        {% endfor %}
      </class>
      <class id="{{ c['subject'] }} {{ c['nr'] }} Lec 1b" type="Lec" suffix="1b" limit="50" studentScheduling="true" displayInScheduleBook="true" cancelled="false">
        {% for i in c['teoria'] %}
        <instructor id="{{i}}" share="{{100//len(c['teoria'])}}"/>
        {% endfor %}
      </class>
      <class id="{{ c['subject'] }} {{ c['nr'] }} Lab 1" type="Lab" suffix="1" limit="25" studentScheduling="true" displayInScheduleBook="true" cancelled="false">
        {% for i in c['laboratorio'] %}
        <instructor id="{{i}}" share="{{100//len(c['laboratorio'])}}"/>
        {% endfor %}
      </class>
      <class id="{{ c['subject'] }} {{ c['nr'] }} Lab 2" type="Lab" suffix="2" limit="25" studentScheduling="true" displayInScheduleBook="true" cancelled="false">
        {% for i in c['laboratorio'] %}
        <instructor id="{{i}}" share="{{100//len(c['laboratorio'])}}"/>
        {% endfor %}
      </class>
    </config>
  </offering>
  {% endfor %}

</offerings>
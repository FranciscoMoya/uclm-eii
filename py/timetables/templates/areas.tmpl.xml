<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE offerings PUBLIC "-//UniTime//DTD University Course Timetabling/EN" "https://www.unitime.org/interface/SubjectArea.dtd">

{% set areas = data %}

<subjectAreas campus="UCLM" term="{{term}}" year="2019">

  {% for a in areas %}
    <subjectArea 
        externalId="{{ a['aid'] }}" 
        abbreviation="{{ a['aid'] }}" 
        title="{{ a['area'] }}" 
        schedBookOnly="false" 
        pseudoSubjArea="false" 
        department="Acad"/>
  {% endfor %}

</subjectAreas>